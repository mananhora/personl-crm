{
  "name": "log4js",
  "version": "2.5.3",
  "description": "Port of Log4js to work with node.",
  "homepage": "https://log4js-node.github.io/log4js-node/",
  "files": [
    "lib",
    "types"
  ],
  "keywords": [
    "logging",
    "log",
    "log4j",
    "node"
  ],
  "license": "Apache-2.0",
